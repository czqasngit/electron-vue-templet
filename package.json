{
    "name": "electron-vue-templete",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "webpack-dev-server",
        "wait-dev-serve": "wait-on http://localhost:8080 && webpack --config webpack-main.config.js",
        "start-electron": "electron ./dist/app.bundle.js",
        "start-dev": "concurrently \"npm run serve\" \"npm run wait-dev-serve && npm run start-electron\"",
        "build": "webpack"
    },
    "dependencies": {
        "vue": "^2.6.10",
        "vue-class-component": "^7.0.2",
        "vue-property-decorator": "^8.3.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-eslint": "^4.1.0",
        "@vue/cli-plugin-typescript": "^4.1.0",
        "@vue/cli-service": "^5.0.8",
        "@vue/eslint-config-typescript": "^4.0.0",
        "clean-webpack-plugin": "^3.0.0",
        "concurrently": "^5.1.0",
        "css-loader": "^3.4.2",
        "electron": "^7.2.4",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "file-loader": "^5.0.2",
        "html-webpack-plugin": "^5.5.0",
        "style-loader": "^1.1.3",
        "ts-loader": "^6.2.1",
        "typescript": "~3.5.3",
        "vue-loader": "^15.8.3",
        "vue-template-compiler": "^2.6.10",
        "wait-on": "^4.0.0",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
    }
}